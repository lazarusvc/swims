@model IEnumerable<SWIMS.Models.SW_form>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.uuid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.desc)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.form)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isApproval_01)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isApproval_02)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.isApproval_03)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.dateModified)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SW_identity)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.uuid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.desc)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.form)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isApproval_01)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isApproval_02)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.isApproval_03)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dateModified)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SW_identity.name)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
